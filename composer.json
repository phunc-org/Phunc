{
  "name": "tom-sapletta-com/phunc",
  "description": "Functional Environment for PHP",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "functional programming",
    "strong typing"
  ],
  "homepage": "http://phunc.de",
  "authors": [
    {
      "name": "Tomasz Sapletta",
      "email": "tom@sapletta.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "nesbot/carbon": "*",
    "codeception/codeception": "*"
  },
  "autoload": {
    "psr-4": {
      "Phunc\\": "src/",
      "Config\\": "config/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "Phunc\\Tests\\": "tests/"
    }
  }
}
