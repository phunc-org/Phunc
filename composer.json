{
  "name": "tom-sapletta-com/phunc",
  "version": "0.1.2",
  "description": "Functional Environment for PHP",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "functional programming",
    "strong typing",
    "Conversion from Array to Collection",
    "searching and filtering in Collection"
  ],
  "homepage": "https://phunc.org",
  "authors": [
    {
      "name": "Tomasz Sapletta",
      "email": "tom@sapletta.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "nesbot/carbon": "*",
    "codeception/codeception": "*"
  },
  "autoload": {
    "psr-4": {
      "Phunc\\": "src/",
      "Config\\": "config/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "Phunc\\Tests\\": "tests/"
    }
  }
}
