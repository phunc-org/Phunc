{
  "name": "phpfunc/phunc",
  "version": "1.0.0",
  "description": "Functional Environment for PHP",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "functional programming",
    "strong typing",
    "Conversion from Array to Collection",
    "searching and filtering in Collection"
  ],
  "homepage": "https://phpfunc.com",
  "authors": [
    {
      "name": "Tomasz Sapletta",
      "email": "tom@sapletta.com"
    }
  ],
  "require": {    
    "nesbot/carbon": "*"    
  },
  "autoload": {
    "psr-4": {
      "Phunc\\": "src/",
      "Config\\": "config/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "Phunc\\Tests\\": "tests/"
    }
  }
}
